<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>

	<settings defaultResolver="chain" />

	<resolvers>
		<chain name="chain">
			<ibiblio name="central" m2compatible="true" />
			<ibiblio name="objectify" m2compatible="true" root="http://maven.objectify-appengine.googlecode.com/git/" />
			<url name="spring.erb.releases">
				<ivy
					pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
				<artifact
					pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>
			<url name="spring.erb.external">
				<ivy
					pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
				<artifact
					pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
 			</url> 
			<ibiblio name="jboss.publib" m2compatible="true" root="https://repository.jboss.org/nexus/content/groups/public" />
			<ibiblio name="mirror" m2compatible="true" root="http://mirrors.ibiblio.org/maven2/" />
		</chain>
	</resolvers>
</ivysettings>